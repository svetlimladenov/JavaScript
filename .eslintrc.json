{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console" :"off",
        "linebreak-style" : "off",
        "indent" : ["warn", 4],
        "quotes":"off",
        "arrow-body-style":"off"
    }
}
